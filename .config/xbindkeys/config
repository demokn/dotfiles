# Run `xbindkeys -f ~/.config/xbindkeys/config --key`
# to identify one key pressed

"xbacklight -inc 5"
    XF86MonBrightnessUp

"xbacklight -dec 5"
    XF86MonBrightnessDown

"pamixer -t"
    XF86AudioMute

"pamixer --allow-boost -d 10"
    XF86AudioLowerVolume

"pamixer --allow-boost -i 10"
    XF86AudioRaiseVolume

"rofi -show run"
    Mod4 + r

"rofi -show drun"
    Mod4+Shift + d

"rofi -show ssh"
    Mod4+Shift + s

"alacritty"
    Mod4+Shift + apostrophe

"dm-editor"
    Control+Alt + e

"dm-browser"
    Control+Alt + b

"dm-wifi"
    Control+Alt + w

"dm-logout"
    Control+Alt + l

"dm-websearch"
    Control+Alt + s

"dm-emoji"
    Control+Alt + u

"dm-shot"
    Control+Alt + a

"flameshot gui"
    Control+Shift + a
