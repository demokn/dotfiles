    Control+Alt + u

"dm-shot"
    Control+Alt + a

"xbacklight -inc 10"
    XF86MonBrightnessUp

"xbacklight -dec 10"
    XF86MonBrightnessDown

"pamixer -t"
    XF86AudioMute

"pamixer --allow-boost -d 10"
    XF86AudioLowerVolume

"pamixer --allow-boost -i 10"
    XF86AudioRaiseVolume

"flameshot gui"
    Control+Shift + a
